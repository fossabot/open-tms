{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "airbnb-typescript/base",
        "prettier",
        "prettier/@typescript-eslint"
    ],
    "rules": {
        "import/prefer-default-export": 0,
        "@typescript-eslint/lines-between-class-members": 0,
        "import/no-cycle": 0
    }
}